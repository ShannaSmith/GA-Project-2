<!DOCTYPE html>
<html>
  <head>
   
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
   
<%- include('./partials/header') %>
<div class="card text-white bg-primary mb-3" style="max-width: 18rem;">
    <div class="card-header">Header</div>
    <div class="card-body">
      <h5 class="card-title">Add Vehicle</h5>
      <form id="new-form" action="/vehicles" methods="POST">
        <div class="form-group">
          <label for="formGroupExampleInput">VIN</label>
          <input type="text" class="form-control" id="formGroupExampleInput" placeholder="vehicle Identifcation number">
        </div>
        <div class="form-group">
          <label for="formGroupExampleInput2">Vehicle nickname</label>
          <input type="text" class="form-control" id="formGroupExampleInput2" placeholder="Dad's truck">
        </div>
        <div class="form-group">
            <label for="formGroupExampleInput2">Year</label>
            <input type="number" min='1908' max='2030' name='year'class="form-control" id="formGroupExampleInput2" placeholder="2020">
          </div>
          <div class="form-group">
            <label for="formGroupExampleInput2">Make</label>
            <input type="text" class="form-control" id="formGroupExampleInput2" placeholder="make of vehicle">
          </div>
          <div class="form-group">
            <label for="formGroupExampleInput2">Model</label>
            <input type="text" class="form-control" id="formGroupExampleInput2" placeholder="make of vehicle">
          </div>
      </form >
      <button type="submit" class="btn btn-success">Add Vehicle</button>

    </div>
  </div>
  </body>
<script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
  <script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
    </body>
  </html>
